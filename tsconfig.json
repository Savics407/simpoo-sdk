{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
     "incremental": true,
  "tsBuildInfoFile": "dist/.tsbuildinfo"
  },
  "include": ["src", "src/types"],
  "exclude": ["node_modules", "dist"]
}
